<%- include("partials/header.ejs") -%>

<h1>Compose</h1>

<form action="/compose" method="post" class="form-group">
    <div class="form-group">
        <label for="title">Title</label>
        <input type="text" name="titleText" autocomplete="off" style="min-width: 100%" class="form-control">    
    </div >
    <div class="form-group">
        <label for="new Text">Post</label>
        <textarea name="newText" id="" style="min-width: 100%" rows="8" autocomplete="off" class="form-control"></textarea>    
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer.ejs") -%>